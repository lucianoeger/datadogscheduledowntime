            "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://github.com/lucianoeger/datadogscheduledowntime"
        },
        "issues": {
            "uri": "https://github.com/lucianoeger/datadogscheduledowntime/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/lucianoeger/datadogscheduledowntime"
    },
    "screenshots": [
        {
            "path": "tasks/datadogscheduledowntime/screenshots/task.png"
